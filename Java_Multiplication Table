package March23;
import java.util.Scanner;
public class Practice01 {
	
	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		System.out.println("Choose number between 2 to 9 for multiplication table");
		
		while (true) {
			int N = scanner.nextInt();
		printing99dan(N);
		}
	}
	
	public static void printing99dan(int num) {
		if (num > 1 && num < 10) {
			for (int i = 1; i < 10; i ++) {
				System.out.printf("%d X %d = %d%n", num, i, num*i);
			}
		} else {
			System.out.println("Try again with a right number");
		}
	}
	
}
